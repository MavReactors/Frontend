{
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": [
      "*.{svg,png,jpg,gif}",
      "*.{css,scss}",
      "*.js"
    ]
  },
  "routes": [
    {
      "route": "/*.js",
      "headers": {
        "content-type": "text/javascript"
      }
    }
  ],
  "mimeTypes": {
    ".json": "text/json"
  }
}